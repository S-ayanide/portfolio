{
  "name": "portfolio",
  "version": "0.1.0",
  "description": "A portfolio website of Sayan Mondal",
  "private": true,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "build": "react-scripts build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/S-ayanide/portfolio"
  },
  "bugs": {
    "url": "https://github.com/S-ayanide/portfolio/issues"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint"
    }
  }
}
